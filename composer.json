{
  "name": "tourze/user-event-bundle",
  "description": "用户交互事件处理和收集系统",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/security-core": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/enum-extra": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\UserEventBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\UserEventBundle\\Tests\\": "tests"
    }
  }
}